{
  "diagnostic.warningSign": ">>",
  "rust-analyzer.check.command": "clippy",
  "workspace.ignoredFolders": [
    "$HOME",
    "$HOME/.cargo/**",
    "$HOME/.rustup/**"
  ],
  "languageserver": {
    "ocaml-lsp": {
      "command": "opam",
      "args": ["config", "exec", "--", "ocamllsp"],
      "filetypes": ["ocaml", "reason"]
    }
  }
}
